def foo(x) -> (y) {
  y = bar(x, x, x)
}

def bar(a, b, c) -> (u) {
  x = bar(a + 1, b + 1, c + 1);
  y, z, w = gab();
  v = bar(gab());
  u = x + y + z + w + v
}

def gab() -> (a, b, c) {
    a = 1;
    b = 2;
    c = 3
}

def fib(x) -> (ret) {
    if (x == 0) || (x == 1) {
        ret = 1
    } else {
        ret = fib(x - 1) + fib(x - 2)
    }
}
