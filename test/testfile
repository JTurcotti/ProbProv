def foo(x) -> (y) {
  y = bar(x, x, x)
}

def bar(a, b, c) -> (u) {
  x = bar(a + 1, b + 1, c + 1);
  y, z, w = gab();
  v = bar(gab());
  u = x + y + z + w + v
}

def gab() -> (a, b, c) {
    a = 0;
    b = 1;
    c = 2
}

def fib(x) -> (ret) {
    if (x == 0) || (x == 1) {
        ret = 1
    } else {
        ret = fib(x - 1) + fib(x - 2)
    }
}

def baz() -> (x, y) {
    z1 = 0;
    z2 = 0;
    if 0 {
        x = z1;
        y = z1
    } else {
        x = z2;
        y = z2
    }
}

def baz_use() -> (x1, y1, x2, y2) {
        x1, y1 = baz();
        x2, y2 = baz()
}
        
