def f(x) -> y {
    if 0 {
        t = 0;
        if x {
            t = 1
        };
        if t {
            return 0
        };
	return 1
    } else {
      return 2
    }
}

def f_classic(x) -> y {
    if 0 {
        t = 0;
        if x {
            t = 1
        };
        y = 0;
        if t {
            y = 1
        }
    } else {
        y = 2
    };
    return y
}
